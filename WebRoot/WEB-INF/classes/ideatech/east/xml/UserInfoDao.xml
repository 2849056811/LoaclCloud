<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ideatech.east.dao.UserInfoDao">
	<!-- login query user information -->
	<select id="selectuserinfodao" resultType="ideatech.east.bean.UserInfo">
		select * from t_userinfo where name=#{uName} and password=#{uPassWord}
	</select>
</mapper>
